{
  "_comment": "Template for FIX_CASCADE sub-orchestrator parameters",
  "sub_orchestrator_type": "FIX_CASCADE",
  "unique_id": "GENERATE_UUID_HERE",
  "master_state_file": "orchestrator-state.json",
  "input_parameters": {
    "fix_id": "fix-ISSUE_ID-UUID",
    "branches_to_fix": [
      "branch1",
      "branch2",
      "branch3"
    ],
    "issue_description": "Detailed description of the issue being fixed",
    "validation_requirements": {
      "tests_must_pass": true,
      "linting_required": true,
      "review_required": false,
      "build_required": true,
      "coverage_threshold": 80
    },
    "source_fix": {
      "branch": "fix/original-fix-branch",
      "commit": "COMMIT_SHA",
      "author": "SW_ENGINEER_NAME",
      "fix_time": "TIMESTAMP"
    },
    "backport_strategy": "CHERRY_PICK|MERGE|REBASE",
    "conflict_resolution": "MANUAL|AUTO_THEIRS|AUTO_OURS",
    "priority": "CRITICAL|HIGH|MEDIUM|LOW"
  },
  "output_location": "/tmp/sub-orch-UUID/output.json",
  "heartbeat_file": "/tmp/sub-orch-UUID/heartbeat.json",
  "checkpoint_file": "/tmp/sub-orch-UUID/checkpoint.json",
  "log_file": "/tmp/sub-orch-UUID.log",
  "max_duration_seconds": 3600,
  "checkpoint_interval": 30,
  "resource_limits": {
    "max_memory_mb": 2048,
    "max_cpu_percent": 80,
    "max_disk_mb": 500
  },
  "monitoring": {
    "heartbeat_interval": 30,
    "progress_update_interval": 60,
    "detailed_logging": false
  },
  "recovery": {
    "max_retries": 3,
    "retry_delay_seconds": 300,
    "checkpoint_on_error": true,
    "preserve_artifacts_on_failure": true
  }
}